generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  name      String @unique
  id        String @id @default(uuid())
  // All position numbers are representing precentage
  positionL Float
  positionT Float
  widthPer  Float
  heightPer Float
}
